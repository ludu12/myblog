{
  "name": "mysite",
  "version": "1.0.0",
  "description": "Keystone JS CMS website",
  "main": "src/server/index.js",
  "author": "Luke Dunscombe",
  "license": "ISC",
  "engines": {
    "node": ">=8.4.0"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build && rimraf ./public",
    "client": "webpack-dev-server --mode development --config config/webpack.dev.config.js --open --hot --history-api-fallback",
    "build": "npm run clean && npm run build:client && npm run build:server && npm run build:keystone",
    "build:client": "webpack --config config/webpack.prod.config.js",
    "build:client:dev": "webpack --config config/webpack.dev.config.js",
    "build:keystone": "cross-env NODE_ENV=production keystone build",
    "build:server": "babel src/server/ --copy-files --out-dir build --source-maps \"inline\"",
    "start": "cross-env NODE_ENV=production keystone start",
    "start:dev": "npm run clean && npm run build:client:dev && npm run build:server && cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev",
    "start:dev:watch": "nodemon --watch src/server/ --exec npm run start:dev"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@keystonejs/adapter-mongoose": "^5.1.3",
    "@keystonejs/app-admin-ui": "^5.3.0",
    "@keystonejs/app-graphql": "^5.0.1",
    "@keystonejs/app-static": "^5.0.0",
    "@keystonejs/auth-password": "^5.0.1",
    "@keystonejs/fields": "^6.0.0",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.0.3",
    "@keystonejs/file-adapters": "^5.3.0",
    "@keystonejs/keystone": "^5.3.0",
    "@material-ui/core": "^4.8.0",
    "babel-plugin-istanbul": "^5.2.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "typeface-roboto": "^0.0.75",
    "webpack": "^4.41.4"
  }
}
