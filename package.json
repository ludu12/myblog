{
  "name": "ludusblog",
  "version": "1.0.0",
  "description": "Keystone JS CMS with React and Redux",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./src/server/public/dist && webpack --config config/webpack.prod.config.js",
    "build:local": "rimraf ./src/server/public/dist && webpack --config config/webpack.local.config.js",
    "lint": "eslint --fix \"src/**/*.js\" \"test/**/*.spec.js\"",
    "test": "mocha",
    "start": "node ./server.js",
    "start:local": "webpack-dev-server --mode development --config config/webpack.local.config.js --open --hot --history-api-fallback",
    "verify": "npm run lint && nyc npm test"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "author": "Luke Dunscombe",
  "license": "ISC",
