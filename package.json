    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.2.0",
